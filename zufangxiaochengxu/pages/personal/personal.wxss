
.personalConatiner{ display: flex; flex-direction: column;}
page{ background: #f6f6f6;}
.personalListPd{padding:50rpx 0rpx 0; margin-bottom: 60rpx;  background: #fff; position: relative; z-index: 1;}
.activeBannerPd{ margin: 28rpx;}
.activeBannerPd image{width: 100%; height: 160rpx;}
.personalBg{ background: #fff;}
.headBg{width: 100%; height: 397rpx; text-align: center; position: relative;}
.headBg image.headBgImg{ width: 100%; height:397rpx;}
.headBg image.headLogo{width: 120rpx; height: 120rpx; border-radius:100%; display: block; margin: 0 auto;}
.headBgPd{ display: flex; flex-direction: column; position: absolute; left:0; top:100rpx; width: 100%; }
.headBg text{ color: #333; font-size: 34rpx;}
.headBg text.dz{ /*width: 168rpx; height: 52rpx; line-height: 52rpx; border: 1px solid #eaeaea;  margin: 24rpx 0 0 30rpx; margin-left: 30rpx;*/display: inline-block; line-height: 120rpx;}
.headBg view.names{text-align: center;line-height: 52rpx; /*margin: 0 0 0 30rpx;*/ display: inline-block;  }
.headBg view.names view{color: #333; font-size: 34rpx; display:block; position: relative; margin-top: 30rpx;}
.headBg view.names view text{ color: #20b7b6; border: 1px solid #20b7b6; border-radius:4px; display:block; padding:4rpx 7rpx; font-size: 20rpx; position: absolute; top: -2rpx; left:50%; line-height: 30rpx; margin-left: 116rpx;}
.headBg view.names image{width: 12rpx; height: 22rpx; margin-left: 5rpx; vertical-align: middle;}
.personalList{ margin: 0 28rpx; padding: 35rpx 0; border-top: 1px solid #eaeaea; }
.personalIcon{ width: 36rpx; height: 36rpx; margin-right: 15rpx; vertical-align: middle; margin-top: -5rpx;}
.personalList text{color: #333; font-size: 30rpx;}
.personalRight{ float: right; width: 15rpx; height: 28rpx; vertical-align: middle; margin-top: 12rpx;}
.editBtnPd{ border: none;}
.editBtnPd button{ background: #fff; color: #333; width: 100%; height: 95rpx; line-height:95rpx; border: none;}
button::after{ border: none; }
/*切换身份*/
.changeMessage{ position: absolute; right: 0; top: 44rpx; z-index: 10; background: #fff; padding: 15rpx 28rpx; color: #20b7b6; font-size: 30rpx; border-top-left-radius:100px; border-bottom-left-radius:100px;}
