/* pages/tabbarList/tabbarList.wxss */
button::after{border: none;}
.tabBarList{ display: flex; flex-direction: row; position: fixed; left: 0; bottom: 0; z-index: 22; width: 100%; background: #fff; border-top: 1px solid #f5f5f5; }
.tabBarList view,.tabBarList navigator,.tabBarList button{ width: 50%; text-align: center;font-size:26rpx; border:none; background:#fff; line-height: 40rpx!important;}
.tabBarList view image,.tabBarList navigator image,.tabBarList button image{ width: 50rpx; height: 50rpx; display: block; margin: 0 auto 5rpx;}
.fix-iphonex-button {
 padding:10rpx 0 40rpx!important;
}