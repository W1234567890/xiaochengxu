/* pages/index/reserve/reserve.wxss */
page{ border-top: none;}
.reservePd{ text-align: center; width:750rpx;height:80rpx; line-height: 80rpx; background:#fff;}
.reservePd view{ color: #8A8E98; font-size: 28rpx; display: inline-block; margin: 0 60rpx; position: relative;}
.reservePd view.cur{ color: #5876AB;}
.reservePd view.cur text{width:30rpx;height:4rpx;background:rgba(88,118,171,1);border-radius:3rpx; position: absolute; left: 50%; bottom:5rpx; margin-left:-20rpx; }
.reservePd view text.tabNum{width:auto;height:auto;background:none;border-radius:3rpx; position: relative; left: auto; bottom:auto; margin-left:4rpx; font-size: 20rpx;}
.noMessagePd{ margin: 90rpx 0 0; text-align: center; color: #8A8E98; font-size: 24rpx;}
.noMessagePd image{ width: 220rpx; height: 220rpx; display: block; margin: 0 auto 16rpx;}
.reserveList{ margin-top: 5rpx;}
.reserveLine{background:rgba(255,255,255,1);box-shadow:0px 2rpx 24rpx 0px rgba(96,147,236,0.2);border-radius:8rpx;border:1px solid rgba(148,170,189,0.1); margin: 25rpx 0; position: relative;}
.reserveLine image{ position: absolute; right: 0; bottom: 0; width:106rpx; height: 104rpx; }
.reserveTitle{width:100%;height:68rpx; line-height: 68rpx; background:#94AABD; color: #fff; font-size: 24rpx; font-weight: bold; position: relative; border-top-left-radius:8rpx;border-top-right-radius:8rpx; }
.reserveTitle text.greyText{ color: #C0C2C7;}
.reserveTitle text.redText{ color: #F17276;}
.reserveTitle text.greyBlueText{ color: #94AABD;}

.reserveWhiteBg{ background: #fff;padding: 10rpx 0;}
.reserveWhiteBg view.reserveWhiteList{ margin: 26rpx 0; color: #8A8E98; font-size: 30rpx; vertical-align: middle; line-height: 30rpx;}
.reserveWhiteBg view.reserveWhiteList text{ display: inline-block;width:110rpx;font-size:22rpx; color: #5C6068;font-weight: bold; line-height: 30rpx; margin-top: -4rpx;}

.comingList{ display: flex; flex-direction: row; padding: 40rpx 0; border-bottom: 1px solid #EEF0F4;}
.comingLeft{ width: 117rpx; text-align: left;}
.comingLeft image{ display: block; width: 40rpx; height: 40rpx; margin:35rpx 0 0 16rpx;}
.comingLeft text{ display: block; margin-top: 14rpx; font-size: 24rpx;}
.comingLeft text.red{ color:#F17276; }
.comingLeft text.grey{ color:#94AABD; }
.comingRight{ width: 573rpx;}
.comingTitle{ color:#001629; font-size: 28rpx; position: relative;}
.comingTitle text{ position: absolute; right: 0; top:3rpx; color: #C0C2C7; font-size: 22rpx;}
.comingMiddle{ color: #5C6068; font-size: 28rpx; padding: 19rpx 0 24rpx;}
.comingLast{ display: flex; flex-direction: row; position: relative;}
.comingLast text{ position: absolute; right: 0; top: 7rpx; font-size: 24rpx;}
.comingLast text.greyLight{ color: #C0C2C7;}
.comingLast text.greyDark{ color: #8A8E98;}
.comingLast view{ display: inline-block; margin-right: 30rpx; width: 124rpx;height: 38rpx;line-height: 38rpx; text-align: center; font-size: 22rpx;border-radius:19px;}
.comingLast view.blue{border:1px solid #5876AB;background: #5876AB; color: #fff;}
.comingLast view.blueLight{ border:1px solid #94AABD;background:rgba(148,170,189,0.1); color: #94AABD;}

/*搜索框*/
.searchList{ position: relative; margin: 15rpx 28rpx 0; /*padding-bottom:15rpx;border-bottom: 1px solid #eaeaea;*/text-align: center}
.searchList input{ border-radius:45rpx; border: 1px solid #DEE0E5; background: #fff;width: 87%; padding:15rpx 0; color: #333; font-size: 30rpx; padding-left: 12%; text-align: left;}
.searchList .searchListImg/*icon*/{ position:absolute;top:50%; margin-top:-20rpx;color:#B2B2B2;z-index:10;left:5%; width: 40rpx; height:40rpx; }
.selectList{/*border-top: 1px solid #eaeaea;*/ position: fixed; left: 0; top: 0; width: 100%; z-index: 10; background: #fff;}

/*列表样式*/
.registerPd{ display: none;}
.registerTitle{ margin: 44rpx 0 16rpx; position: relative; color: #8A8E98; font-size: 22rpx; min-height: 40rpx;}
.registerTitle view{ position: absolute; right: 0; top: 0; color: #C0C2C7;}
.registerTitle view image{ width:40rpx; height: 40rpx; vertical-align: middle; margin-top: -6rpx;}
.registerList{ margin: 0 0 20rpx; padding: 34rpx 30rpx; display: flex; flex-direction: row; background:rgba(255,255,255,1);box-shadow:0px 0px 16rpx 0px rgba(96,147,236,0.2);border-radius:4px;}
.registerLeft{width:100rpx;height:100rpx; margin:24rpx 24rpx 0 0; line-height: 100rpx; text-align: center; background:rgba(249,249,250,1); border-radius: 100%; border:1px solid rgba(222,224,229,1); color: #94AABD; font-size: 42rpx;}
.registerRight{ width: 506rpx;}
.rightTitle{color: #001629;font-size: 32rpx;}
.rightData{color: #8A8E98;font-size: 24rpx; margin: 30rpx 0 4rpx;}
.rightAddress{color: #8A8E98;font-size: 24rpx;overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}

.addRegisterBtn{width:518rpx;height:72rpx; line-height: 72rpx;text-align: center; background:rgba(88,118,171,1);box-shadow:0px 8rpx 20rpx 0px rgba(96,147,236,0.5);border-radius:36rpx; position:fixed; left: 50%; bottom: 100rpx; margin-left: -259rpx; color: #fff; font-size: 32rpx;}
.addRegisterBtn image{ width: 24rpx; height: 24rpx; vertical-align: middle; margin:-5rpx 11rpx 0 0;}