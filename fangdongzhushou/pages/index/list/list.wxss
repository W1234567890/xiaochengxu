page{ border-top: none;background:rgb(238,240,244);}
.searchConaiver{display: flex; flex-direction: column; /*border-top: 1px solid #eaeaea; padding-top: 85rpx;*/}
.searchList{ position: relative; margin: 15rpx 28rpx 0; /*padding-bottom:15rpx;border-bottom: 1px solid #eaeaea;*/text-align: center; height: 84rpx;}
.searchList input.searchInput{ border-radius:45rpx; border: 1px solid #DEE0E5; background: #fff;width: 87%; padding:15rpx 0; color: #333; font-size: 30rpx; padding-left: 13%; text-align: left; position: absolute; right: 0; top: 0; z-index: 10;}
