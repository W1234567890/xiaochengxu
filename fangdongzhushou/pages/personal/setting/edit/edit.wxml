<view class="editPd">
  <view class='personalLogo'>头像
    <image src='{{brand_logo}}' mode='aspectFill' catchtap="chooseImageTap"></image>
  </view>
  <view style='padding:40rpx 0;'>昵称
    <input type='text' placeholder='{{nickname}}' placeholder-class='placeHolderCss' bindinput='nicknameTap' value='{{nickname}}'/>
  </view>  
  <button class="submitBtn" bindtap='brandlogoTap'>保存</button>
</view>
